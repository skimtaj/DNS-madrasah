<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>Admin Dashboard | Program Management</title>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.0.1/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.7.1/chart.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.0.1/css/buttons.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.2.9/css/responsive.dataTables.min.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>


    <style>
        @import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap');

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
        }

        :root {
            --primaryColor: #2c3e50;
            --primaryLight: #34495e;
            --primaryLighter: #7f8c8d;
            --primaryDark: #1a252f;
            --secondaryColor: #27ae60;
            --dangerColor: #e74c3c;
            --warningColor: #f39c12;
            --infoColor: #3498db;
            --whiteColor: #FFFFFF;
            --lightGray: #f5f6f7;
            --mediumGray: #ecf0f1;
            --darkGray: #95a5a6;
            --textColor: #2c3e50;
            --borderColor: #bdc3c7;
            --smallBorder: 6px;
            --mediumBorder: 10px;
            --bigBorder: 14px;
            --shadow-sm: 0 1px 2px 0 rgba(0, 0, 0, 0.05);
            --shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06);
            --shadow-md: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);
            --shadow-lg: 0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05);
            --shadow-xl: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04);
            --fs13: 0.8125rem;
            --fs14: 0.875rem;
            --fs15: 0.9375rem;
            --fs16: 1rem;
            --fs18: 1.125rem;
            --fs20: 1.25rem;
            --fs24: 1.5rem;
            --fs30: 1.875rem;
            --fs36: 2.25rem;
            --sidebarWidth: 250px;
            --headerHeight: 70px;
        }

        html {
            font-size: 16px;
            scroll-behavior: smooth;
        }

        body {
            position: relative;
            color: var(--textColor);
            background-color: var(--lightGray);
            min-height: 100vh;
            overflow-x: hidden;
            line-height: 1.5;
        }

        .sidebar-placeholder::after {
            content: "Sidebar Area - Will be attached later";
            display: block;
            padding: 1rem;
            background: #f0f0f0;
            border-radius: 8px;
            margin-bottom: 1rem;
            text-align: center;
            color: #666;
        }

        h1,
        h2,
        h3,
        h4,
        h5,
        h6 {
            line-height: 1.3;
            font-weight: 600;
            color: var(--textColor);
        }

        ul {
            list-style: none;
        }

        a {
            text-decoration: none;
            color: inherit;
        }

        ::-webkit-scrollbar {
            width: 6px;
            height: 6px;
        }

        ::-webkit-scrollbar-track {
            background-color: var(--lightGray);
        }

        ::-webkit-scrollbar-thumb {
            background-color: var(--primaryLight);
            border-radius: 3px;
        }

        .container {
            height: 100%;
            padding: 1.5rem;
            transition: all 0.3s ease;
            position: relative;
            width: calc(100% - var(--sidebarWidth));
            margin-left: var(--sidebarWidth);
        }

        .main {
            display: grid;
            gap: 1.5rem;
            width: 100%;
            margin-top: 5px;
            grid-template-columns: repeat(12, 1fr);
            grid-auto-rows: minmax(150px, auto);
        }

        .main .box {
            padding: 1.5rem;
            background-color: var(--whiteColor);
            border-radius: var(--mediumBorder);
            box-shadow: var(--shadow);
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
            width: 100%;
        }

        .box-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 1.5rem;
        }

        .box-title {
            font-size: var(--fs18);
            font-weight: 600;
            color: var(--textColor);
        }

        .box-actions {
            display: flex;
            gap: 0.5rem;
        }

        .box-action-btn {
            width: 32px;
            height: 32px;
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: var(--smallBorder);
            background-color: var(--lightGray);
            color: var(--darkGray);
            transition: all 0.2s ease;
            cursor: pointer;
            font-size: var(--fs14);
        }

        .box-action-btn:hover {
            background-color: var(--mediumGray);
            color: var(--textColor);
        }

        .admin-profile-section {
            grid-column: span 12;
            display: flex;
            align-items: center;
            padding: 1.25rem;
            background: linear-gradient(rgba(44, 62, 80, 0.85), rgba(52, 73, 94, 0.9)),
                url('https://www.lg.com/ae/images/microsite/your-guide-to-choosing-the-right-air-conditioner/LG-Article-Air-Conditioner-Type-2023-03-M.jpg') center/cover no-repeat;
            border-radius: var(--mediumBorder);
            box-shadow: var(--shadow-lg);
            margin-bottom: 1.5rem;
            gap: 1.25rem;
            flex-wrap: wrap;
            color: white;
            position: relative;
            overflow: hidden;
            min-height: 160px;
        }

        .admin-profile-section::before {
            content: '';
            position: absolute;
            top: -50%;
            right: -50%;
            width: 100%;
            height: 200%;
            background: rgba(255, 255, 255, 0.05);
            transform: rotate(-15deg);
            z-index: 1;
        }

        .admin-profile-img-container {
            position: relative;
            z-index: 2;
            flex-shrink: 0;
        }

        .admin-profile-img {
            width: 70px;
            height: 70px;
            border-radius: 50%;
            object-fit: cover;
            border: 3px solid rgba(255, 255, 255, 0.3);
            box-shadow: var(--shadow-md);
        }

        .admin-status-indicator {
            position: absolute;
            bottom: 5px;
            right: 5px;
            width: 14px;
            height: 14px;
            background-color: var(--secondaryColor);
            border: 2px solid white;
            border-radius: 50%;
        }

        .admin-profile-info {
            flex: 1;
            min-width: 200px;
            z-index: 2;
        }

        .admin-profile-name {
            font-size: var(--fs18);
            font-weight: 700;
            margin-bottom: 0.5rem;
            color: white;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .admin-profile-badge {
            font-size: var(--fs13);
            background: rgba(255, 255, 255, 0.2);
            padding: 0.2rem 0.4rem;
            border-radius: 20px;
            font-weight: 500;
        }

        .admin-profile-details {
            display: flex;
            flex-direction: column;
            gap: 0.25rem;
        }

        .admin-profile-detail {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            font-size: var(--fs13);
            color: rgba(255, 255, 255, 0.9);
        }

        .admin-profile-detail i {
            width: 14px;
            color: rgba(255, 255, 255, 0.8);
            font-size: var(--fs13);
        }

        .admin-profile-actions {
            display: flex;
            align-items: center;
            gap: 0.75rem;
            z-index: 2;
        }

        .btn-update-profile {
            padding: 0.5rem 1rem;
            background-color: rgba(255, 255, 255, 0.15);
            backdrop-filter: blur(10px);
            color: white;
            border: 1px solid rgba(255, 255, 255, 0.2);
            border-radius: var(--smallBorder);
            font-weight: 600;
            cursor: pointer;
            transition: all 0.2s ease;
            display: flex;
            align-items: center;
            gap: 0.5rem;
            white-space: nowrap;
            font-size: var(--fs13);
        }

        .btn-update-profile:hover {
            background-color: rgba(255, 255, 255, 0.25);
            transform: translateY(-2px);
            box-shadow: var(--shadow-md);
        }

        .stats-box {
            display: flex;
            flex-direction: column;
            align-items: flex-start;
            justify-content: space-between;
            min-height: 130px;
            padding: 1.25rem;
            border-left: 4px solid var(--primaryColor);
            position: relative;
            overflow: hidden;
            transition: transform 0.3s ease, box-shadow 0.3s ease;
            width: 100%;
        }

        .stats-box:hover {
            transform: translateY(-5px);
            box-shadow: var(--shadow-lg);
        }

        .stats-box::before {
            content: '';
            position: absolute;
            top: 0;
            right: 0;
            width: 70px;
            height: 70px;
            background-color: rgba(0, 0, 0, 0.03);
            border-radius: 50%;
            transform: translate(30%, -30%);
        }

        .stats-box .stats-value {
            font-size: var(--fs24);
            font-weight: 700;
            margin-bottom: 0.25rem;
            color: var(--textColor);
        }

        .stats-box .stats-label {
            text-transform: uppercase;
            font-size: var(--fs13);
            font-weight: 500;
            color: var(--darkGray);
            letter-spacing: 0.5px;
            display: block;
        }

        .stats-box .stats-icon {
            font-size: 2rem;
            color: var(--primaryColor);
            opacity: 0.8;
            margin-top: 0.5rem;
            align-self: flex-end;
        }

        .stats-box.trend-up .stats-value {
            color: var(--secondaryColor);
        }

        .stats-box.trend-down .stats-value {
            color: var(--dangerColor);
        }

        .stats-box.secondary {
            border-left-color: var(--secondaryColor);
        }

        .stats-box.secondary .stats-icon {
            color: var(--secondaryColor);
        }

        .stats-box.danger {
            border-left-color: var(--dangerColor);
        }

        .stats-box.danger .stats-icon {
            color: var(--dangerColor);
        }

        .stats-box.warning {
            border-left-color: var(--warningColor);
        }

        .stats-box.warning .stats-icon {
            color: var(--warningColor);
        }

        .trend-indicator {
            display: inline-flex;
            align-items: center;
            font-size: var(--fs13);
            font-weight: 500;
            padding: 0.2rem 0.4rem;
            border-radius: var(--smallBorder);
            margin-left: 0.5rem;
        }

        .trend-up {
            background-color: rgba(39, 174, 96, 0.1);
            color: var(--secondaryColor);
        }

        .trend-down {
            background-color: rgba(231, 76, 60, 0.1);
            color: var(--dangerColor);
        }

        .trend-neutral {
            background-color: rgba(149, 165, 166, 0.1);
            color: var(--darkGray);
        }

        .tooltip-inner {
            font-size: var(--fs13);
            padding: 0.375rem 0.75rem;
            background-color: var(--textColor);
        }

        .bs-tooltip-auto[data-popper-placement^=top] .tooltip-arrow::before,
        .bs-tooltip-top .tooltip-arrow::before {
            border-top-color: var(--textColor);
        }

        .data-table-section {
            width: 100%;
            padding: 1.25rem;
            background-color: var(--whiteColor);
            border-radius: var(--mediumBorder);
            box-shadow: var(--shadow);
            overflow: hidden;
        }

        .data-table-section .section-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 1rem;
        }

        .data-table-section .section-title {
            font-size: var(--fs18);
            font-weight: 600;
            color: var(--textColor);
        }

        .data-table-section .section-actions {
            display: flex;
            gap: 0.5rem;
        }

        .filter-controls {
            display: flex;
            flex-wrap: wrap;
            gap: 1rem;
            margin-bottom: 1.5rem;
            padding: 1rem;
            background-color: var(--lightGray);
            border-radius: var(--smallBorder);
        }

        .filter-group {
            display: flex;
            flex-direction: column;
            min-width: 200px;
            flex-grow: 1;
        }

        .filter-label {
            font-size: var(--fs13);
            font-weight: 500;
            color: var(--darkGray);
            margin-bottom: 0.5rem;
            text-transform: uppercase;
            letter-spacing: 0.5px;
        }

        .filter-select {
            padding: 0.625rem 1rem;
            border: 1px solid var(--borderColor);
            border-radius: var(--smallBorder);
            font-size: var(--fs14);
            color: var(--textColor);
            background-color: var(--whiteColor);
            transition: all 0.2s ease;
            appearance: none;
            background-image: url("data:image/svg+xml;charset=UTF-8,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round'%3e%3cpolyline points='6 9 12 15 18 9'%3e%3c/polyline%3e%3c/svg%3e");
            background-repeat: no-repeat;
            background-position: right 0.75rem center;
            background-size: 1rem;
        }

        .filter-select:focus {
            outline: none;
            border-color: var(--primaryLight);
            box-shadow: 0 0 0 3px rgba(52, 73, 94, 0.1);
        }

        .filter-actions {
            display: flex;
            align-items: flex-end;
            gap: 0.5rem;
        }

        .filter-btn {
            padding: 0.625rem 1.25rem;
            border-radius: var(--smallBorder);
            font-size: var(--fs14);
            font-weight: 500;
            cursor: pointer;
            transition: all 0.2s ease;
            border: none;
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
        }

        .filter-btn.apply {
            background-color: var(--primaryColor);
            color: var(--whiteColor);
        }

        .filter-btn.apply:hover {
            background-color: var(--primaryDark);
        }

        .filter-btn.reset {
            background-color: var(--lightGray);
            color: var(--darkGray);
        }

        .filter-btn.reset:hover {
            background-color: var(--mediumGray);
        }

        .table-responsive {
            width: 100%;
            overflow-x: auto;
            -webkit-overflow-scrolling: touch;
        }

        .table {
            width: 100%;
            border-collapse: collapse;
            font-size: var(--fs14);
        }

        .table th {
            background-color: var(--primaryColor);
            color: var(--whiteColor);
            padding: 0.875rem 1rem;
            text-align: left;
            font-weight: 500;
            text-transform: uppercase;
            font-size: var(--fs13);
            letter-spacing: 0.5px;
        }

        .table td {
            padding: 0.875rem 1rem;
            border-bottom: 1px solid var(--borderColor);
            vertical-align: middle;
            color: var(--textColor);
        }

        .table tr:nth-child(even) {
            background-color: var(--lightGray);
        }

        .table tr:hover {
            background-color: rgba(52, 73, 94, 0.03);
        }

        .status-badge {
            display: inline-block;
            padding: 0.25rem 0.75rem;
            border-radius: 50px;
            font-size: var(--fs13);
            font-weight: 500;
        }

        .status-active {
            background-color: rgba(39, 174, 96, 0.1);
            color: var(--secondaryColor);
        }

        .status-pending {
            background-color: rgba(243, 156, 18, 0.1);
            color: var(--warningColor);
        }

        .status-inactive {
            background-color: rgba(149, 165, 166, 0.1);
            color: var(--darkGray);
        }

        .dataTables_wrapper {
            width: 100%;
        }

        .dataTables_wrapper .dataTables_filter {
            margin-bottom: 1rem;
        }

        .dataTables_wrapper .dataTables_filter input {
            padding: 0.5rem 0.75rem;
            border: 1px solid var(--borderColor);
            border-radius: var(--smallBorder);
            margin-left: 0.5rem;
            font-size: var(--fs14);
            transition: all 0.2s ease;
        }

        .dataTables_wrapper .dataTables_filter input:focus {
            border-color: var(--primaryLight);
            outline: none;
            box-shadow: 0 0 0 3px rgba(52, 73, 94, 0.1);
        }

        .dataTables_wrapper .dataTables_length select {
            padding: 0.375rem 1.75rem 0.375rem 0.75rem;
            border: 1px solid var(--borderColor);
            border-radius: var(--smallBorder);
            font-size: var(--fs14);
        }

        .dataTables_wrapper .dataTables_paginate {
            margin-top: 1.5rem;
            padding-top: 1rem;
            display: flex;
            justify-content: center;
            align-items: center;
            gap: 0.25rem;
            flex-wrap: nowrap;
            border-top: 1px solid var(--borderColor);
            overflow-x: auto;
            white-space: nowrap;
        }

        .dataTables_wrapper .dataTables_paginate .paginate_button {
            min-width: 32px;
            height: 32px;
            display: inline-flex;
            align-items: center;
            justify-content: center;
            padding: 0 0.5rem;
            border-radius: var(--smallBorder);
            border: 1px solid var(--borderColor);
            color: var(--textColor);
            font-size: var(--fs14);
            font-weight: 500;
            transition: all 0.2s ease;
            flex-shrink: 0;
        }

        .dataTables_wrapper .dataTables_paginate .paginate_button:hover {
            background-color: var(--lightGray);
            border-color: var(--mediumGray);
            color: var(--primaryColor);
        }

        .dataTables_wrapper .dataTables_paginate .paginate_button.current {
            background-color: var(--primaryColor);
            border-color: var(--primaryColor);
            color: var(--whiteColor);
            font-weight: 600;
        }

        .dataTables_wrapper .dataTables_paginate .paginate_button.disabled {
            opacity: 0.5;
            pointer-events: none;
        }

        .dataTables_wrapper .dataTables_info {
            font-size: var(--fs14);
            color: var(--darkGray);
            padding-top: 1rem;
        }

        .view-details-btn {
            margin-top: 10px;
            padding: 4px 14px;
            background: #463ACC;
            color: #fff;
            border: none;
            border-radius: 6px;
            font-size: 12px;
            cursor: pointer;
            transition: all 0.2s ease;
        }

        .view-details-btn:hover {
            background: #3529A3;
            transform: translateY(-2px);
            box-shadow: var(--shadow-sm);
        }

        @media (min-width: 576px) {
            .container {
                padding: 1.5rem;
            }

            .stats-box {
                flex-direction: row;
                align-items: center;
            }

            .stats-box .stats-icon {
                margin-top: 0;
                font-size: 2.5rem;
            }
        }

        @media (min-width: 768px) {
            .main {
                grid-template-columns: repeat(2, 1fr);
                gap: 1.5rem;
            }

            .data-table-section {
                grid-column: span 2;
            }
        }

        @media (min-width: 992px) {
            .container {
                padding: 1.5rem;
            }

            .main {
                grid-template-columns: repeat(12, 1fr);
                grid-auto-rows: minmax(130px, auto);
                gap: 1.25rem;
            }

            .stats-box {
                grid-column: span 3;
                grid-row: 2;
            }

            .data-table-section {
                grid-column: span 12;
            }
        }

        @media (min-width: 1200px) {
            .container {
                max-width: calc(1400px - var(--sidebarWidth));
                margin: 0 auto;
                margin-left: var(--sidebarWidth);
            }
        }

        @media (max-width: 575.98px) {
            html {
                font-size: 14px;
            }

            .container {
                padding: 1rem;
                margin-left: 0 !important;
                width: 100% !important;
            }

            .main {
                display: flex;
                flex-direction: column;
                gap: 1rem;
            }

            .box,
            .data-table-section {
                width: 100%;
                margin-left: 0;
                margin-right: 0;
                padding: 1rem;
            }

            .admin-profile-section {
                flex-direction: column;
                text-align: left;
                padding: 1rem;
                gap: 0.75rem;
                align-items: flex-start;
                min-height: 140px;
            }

            .admin-profile-img {
                width: 50px;
                height: 50px;
            }

            .admin-profile-info {
                width: 100%;
            }

            .admin-profile-details {
                align-items: flex-start;
            }

            .admin-profile-actions {
                justify-content: flex-start;
                width: 100%;
                flex-wrap: wrap;
            }

            .btn-update-profile {
                padding: 0.4rem 0.8rem;
                font-size: var(--fs13);
            }

            .admin-profile-name {
                font-size: 16px;
            }

            .stats-box {
                min-height: 110px;
                padding: 1rem;
            }

            .stats-box .stats-value {
                font-size: var(--fs20);
            }

            .stats-box .stats-icon {
                font-size: 1.75rem;
            }

            .table th,
            .table td {
                padding: 0.5rem;
                font-size: var(--fs13);
            }

            .dataTables_wrapper .dataTables_length,
            .dataTables_wrapper .dataTables_filter {
                width: 100%;
                margin-bottom: 0.5rem;
            }

            .dataTables_wrapper .dataTables_filter input {
                width: 100%;
                margin-left: 0;
                margin-top: 0.5rem;
            }

            /* Mobile pagination adjustments */
            .dataTables_wrapper .dataTables_paginate {
                gap: 0.125rem;
            }

            .dataTables_wrapper .dataTables_paginate .paginate_button {
                min-width: 28px;
                height: 28px;
                font-size: var(--fs13);
                padding: 0 0.25rem;
            }

            /* Filter controls on mobile */
            .filter-controls {
                flex-direction: column;
                gap: 0.75rem;
            }

            .filter-group {
                min-width: 100%;
            }
        }

        @media (max-width: 991.98px) {
            .container {
                margin-left: 0 !important;
                width: 100% !important;
            }

            .admin-profile-section {
                flex-direction: row;
                flex-wrap: wrap;
                padding: 1rem;
                min-height: 150px;
            }

            .admin-profile-info {
                min-width: auto;
                flex: 1 1 60%;
            }

            .admin-profile-actions {
                flex: 1 1 100%;
                justify-content: flex-start;
                margin-top: 0.5rem;
            }
        }

        .fade-in {
            animation: fadeIn 0.5s ease-in-out;
        }

        @keyframes fadeIn {
            from {
                opacity: 0;
                transform: translateY(10px);
            }

            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .pulse {
            animation: pulse 2s infinite;
        }

        .dropdown-toggle::after {
            display: none;
        }

        @keyframes pulse {
            0% {
                box-shadow: 0 0 0 0 rgba(44, 62, 80, 0.4);
            }

            70% {
                box-shadow: 0 0 0 10px rgba(44, 62, 80, 0);
            }

            100% {
                box-shadow: 0 0 0 0 rgba(44, 62, 80, 0);
            }
        }
    </style>
</head>

<body>


    <% if (messages.success) { %>
        <script>
            Swal.fire({
                toast: true,
                position: "top-end",
                icon: "success",
                title: "<%= messages.success %>",
                showConfirmButton: false,
                timer: 3000,
                timerProgressBar: true,
                background: "#ffffff",
                iconColor: "#00b894",
                color: "#333",
                customClass: {
                    popup: 'custom-swal-toast'
                },
                didOpen: (toast) => {
                    toast.style.boxShadow = '0 4px 12px rgba(0,0,0,0.1)';
                    toast.style.borderRadius = '8px';
                    toast.style.padding = '0.75rem 1.25rem';
                }
            });
        </script>
        <% } %>


            <% if (messages.error) { %>
                <script>
                    Swal.fire({
                        toast: true,
                        position: "top-end",
                        icon: "error",
                        title: "<%= messages.error %>",
                        showConfirmButton: false,
                        timer: 3000,
                        timerProgressBar: true,
                        background: "#ffffff",
                        iconColor: "#red",
                        color: "#333",
                        customClass: {
                            popup: 'custom-swal-toast'
                        },
                        didOpen: (toast) => {
                            toast.style.boxShadow = '0 4px 12px rgba(0,0,0,0.1)';
                            toast.style.borderRadius = '8px';
                            toast.style.padding = '0.75rem 1.25rem';
                        }
                    });
                </script>
                <% } %>



                    <div class="sidebar-placeholder">

                        <%- include('admin_sidebar') %>
                    </div>

                    <div class="container" id="top">
                        <main class="content">
                            <section class="main">
                                <div class="admin-profile-section fade-in" style="animation-delay: 0.05s;">
                                    <div class="admin-profile-img-container">
                                        <img src="https://img.freepik.com/premium-vector/avatar-profile-icon-flat-style-male-user-profile-vector-illustration-isolated-background-man-profile-sign-business-concept_157943-38764.jpg"
                                            alt="Admin Profile" class="admin-profile-img">
                                        <div class="admin-status-indicator"></div>
                                    </div>
                                    <div class="admin-profile-info">
                                        <h3 class="admin-profile-name">
                                            <%=adminSourse.name%> <span class="admin-profile-badge">Administrator</span>
                                        </h3>
                                        <div class="admin-profile-details">
                                            <div class="admin-profile-detail">
                                                <i class="fas fa-phone"></i>
                                                <span>
                                                    <%=adminSourse.mobile%>
                                                </span>
                                            </div>
                                            <div class="admin-profile-detail">
                                                <i class="fas fa-envelope"></i>
                                                <span>
                                                    <%=adminSourse.email%>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="admin-profile-actions">
                                        <button onclick="window.location.href='/coolmate/admin/update-profile'"
                                            class="btn-update-profile">
                                            <i class="fas fa-user-edit"></i>
                                            Update Profile
                                        </button>
                                    </div>
                                </div>

                                <div class="box stats-box fade-in" style="animation-delay: 0.1s;">
                                    <div class="stats-details">
                                        <h4 class="stats-value">
                                            <%=todayEarning%>
                                                <span class="trend-indicator trend-up">
                                                    <i class="fas fa-arrow-up"></i> 5.2%
                                                </span>
                                        </h4>
                                        <span class="stats-label">Today's Earning</span>
                                        <button onclick="window.location.href='/coolmate/admin/report'" class="view-details-btn">
                                            View Details
                                        </button>
                                    </div>

                                </div>

                                <div class="box stats-box secondary fade-in" style="animation-delay: 0.2s;">
                                    <div class="stats-details">
                                        <h4 class="stats-value">
                                            <%=totalTodayService%> <span class="trend-indicator trend-up"><i
                                                        class="fas fa-arrow-up"></i> 5.2%</span>
                                        </h4>
                                        <span class="stats-label">Today's Job</span>
                                        <button class="view-details-btn">
                                            View Details
                                        </button>
                                    </div>

                                </div>

                                <div class="box stats-box warning fade-in" style="animation-delay: 0.3s;">
                                    <div class="stats-details">
                                        <h4 class="stats-value">
                                            <%=totalpendingService%> <span class="trend-indicator trend-down"><i
                                                        class="fas fa-arrow-down"></i> 3.8%</span>
                                        </h4>
                                        <span class="stats-label">Pending Booking</span>
                                        <button class="view-details-btn">
                                            View Details
                                        </button>
                                    </div>

                                </div>

                                <div class="box stats-box danger fade-in" style="animation-delay: 0.4s;">
                                    <div class="stats-details">
                                        <h4 class="stats-value">87 <span class="trend-indicator trend-neutral">0%</span>
                                        </h4>
                                        <span class="stats-label">Cancelled</span>
                                        <button class="view-details-btn">
                                            View Details
                                        </button>
                                    </div>

                                </div>

                                <!-- Data Table Section -->
                                <div class="data-table-section fade-in" style="animation-delay: 0.5s;">
                                    <div class="section-header">
                                        <h3 class="section-title">Last 7 day's Biling</h3>
                                        <div class="section-actions">
                                            <a href="#" class="box-action-btn" title="Add New"
                                                style='text-decoration : none'>
                                                <i class="fas fa-plus"></i>
                                            </a>

                                            <a href="#" class="box-action-btn" title="Export in excel"
                                                style='text-decoration : none'>
                                                <i class="fas fa-file-excel"></i>
                                            </a>

                                            <a href="#" class="box-action-btn" title="PDF"
                                                style='text-decoration : none'>
                                                <i class="fas fa-file-pdf"></i>
                                            </a>
                                        </div>
                                    </div>

                                    <!-- Filter Controls -->
                                    

                                    <div class="table-responsive">
                                        <table id="monthlyEarning" class="table table-striped">
                                            <thead>
                                                <tr>
                                                    <th>SL No</th>
                                                    <th>Customer</th>
                                                    <th>Mobile</th>
                                                    <th>Address</th>
                                                    <th>Invoice Date</th>
                                                    <th> Grand Total</th>
                                                </tr>
                                            </thead>
                                            <tbody>

                                                <% lastWeekBiling.forEach((lb, index)=>{ %>

                                                    <tr>
                                                        <td>
                                                            <%=index+1%>
                                                        </td>
                                                        <td>
                                                            <%=lb.customerName%>
                                                        </td>
                                                        <td>
                                                            <%=lb.customerPhone%>
                                                        </td>

                                                        <td>

                                                            <%=lb.customerAddress%>
                                                        </td>

                                                        <td>
                                                            <%=lb.invoiceDate%>
                                                        </td>

                                                        <td>
                                                            <%=lb.grandTotal%>
                                                        </td>
                                                    </tr>

                                                    <% }) %>

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </section>
                        </main>
                    </div>
</body>

<script>
    $(document).ready(function () {
        // Initialize DataTable with responsive settings
        $('#monthlyEarning').DataTable({
            responsive: true,
            paging: true,
            searching: true,
            info: true,
            autoWidth: false,
            lengthMenu: [[5, 10, 25, 50, -1], [5, 10, 25, 50, "All"]],
            dom: '<"top"lf>rt<"bottom"ip>',
            language: {
                search: "_INPUT_",
                searchPlaceholder: "Search programs...",
                lengthMenu: "Show _MENU_ entries",
                info: "Showing _START_ to _END_ of _TOTAL_ entries",
                infoEmpty: "Showing 0 to 0 of 0 entries",
                infoFiltered: "(filtered from _MAX_ total entries)",
                paginate: {
                    first: "First",
                    last: "Last",
                    next: "Next",
                    previous: "Previous"
                }
            }
        });
    });
</script>

</html>